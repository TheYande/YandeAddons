{
  "name": "Block palette category icons",
  "description": "Adds icons inside the colored circles that identify block categories.",
  "tags": ["editor", "codeEditor", "theme", "featured"],
  "enabledByDefault": false,
  "dynamicEnable": true,
  "dynamicDisable": true,
  "userstyles": [
    {
      "url": "userstyle.css",
      "matches": ["projects"]
    },
    {
      "url": "category.css",
      "matches": ["projects"],
      "if": { "settings": { "Category": true } }
    }
  ],
  "settings": [
    {
      "name": "Remove Category Label",
      "id": "Category",
      "type": "boolean",
      "default": true
    }
  ],
  "versionAdded": "1.11.0",
  "credits": [
    { "name": "--Explosion--", "link": "https://github.com/explosion-scratch" },
    { "name": "RedGuy7", "link": "https://github.com/RedGuy12" }
  ]
}
